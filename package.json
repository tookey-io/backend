{
  "name": "@tookey/backend",
  "private": true,
  "version": "3.0.0",
  "description": "Tookey backend for User management and expose api",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "prepare": "husky install",
    "commit": "cz",
    "release": "standard-version",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm": "typeorm-ts-node-commonjs -d libs/database/src/typeorm.config.ts",
    "typeorm:generate": "npm run typeorm migration:generate",
    "typeorm:run": "npm run typeorm migration:run",
    "typeorm:revert": "npm run typeorm migration:revert",
    "typeorm:create": "npm run typeorm migration:create",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/app/test/jest-e2e.json"
  },
  "dependencies": {
    "@activepieces/shared": "^0.3.46",
    "@golevelup/nestjs-graphql-request": "^0.1.13",
    "@golevelup/nestjs-rabbitmq": "^3.4.0",
    "@google-cloud/local-auth": "^2.1.1",
    "@metamask/eth-sig-util": "^5.0.2",
    "@nestjs/axios": "^1.0.1",
    "@nestjs/bull": "^0.6.2",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/event-emitter": "^1.3.1",
    "@nestjs/graphql": "^10.1.7",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.2.1",
    "@nestjs/swagger": "^6.1.3",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.2.1",
    "@tookey-io/libtss-ethereum": "^1.2.4",
    "bcrypt": "^5.1.0",
    "bull": "^4.10.2",
    "cache-manager": "^4.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "crypto": "^1.0.1",
    "date-fns": "^2.29.3",
    "discord.js": "^14.7.1",
    "ethers": "^5.7.2",
    "fast-json-stable-stringify": "^2.1.0",
    "fast-memoize": "^2.5.2",
    "firebase-admin": "^11.9.0",
    "google-auth-library": "^9.1.0",
    "graphql-request": "^5.1.0",
    "lodash": "^4.17.21",
    "nestjs-ethers": "^1.0.3",
    "nestjs-pino": "^3.1.1",
    "nestjs-telegraf": "^2.6.0",
    "passport": "^0.6.0",
    "passport-discord": "^0.1.4",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "pino-http": "^8.2.1",
    "pino-pretty": "^9.1.1",
    "qrcode": "^1.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "semver": "^7.5.4",
    "swagger-ui-express": "^4.6.0",
    "telegraf": "^4.8.6",
    "tinycolor2": "^1.5.2",
    "twitter-api-v2": "^1.12.9",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@golevelup/ts-jest": "^0.3.4",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^4.10.0",
    "@types/cache-manager": "^4.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.4",
    "@types/lodash": "^4.14.200",
    "@types/node": "^18.0.3",
    "@types/passport-discord": "^0.1.6",
    "@types/passport-jwt": "^3.0.7",
    "@types/qrcode": "^1.5.0",
    "@types/supertest": "^2.0.12",
    "@types/tinycolor2": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "^28.1.2",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "standard-version": "^9.5.0",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@tookey/database(|/.*)$": "<rootDir>/libs/database/src/$1",
      "^@tookey/access(|/.*)$": "<rootDir>/libs/access/src/$1",
      "^@tookey/amqp(|/.*)$": "<rootDir>/libs/amqp/src/$1"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
